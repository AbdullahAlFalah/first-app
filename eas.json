{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": false,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "GOOGLE_SERVICES_JSON" 
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }       
  },
  "submit": {
    "production": {}
  }
}
